#!/usr/bin/env bash

find . \
  -not \( -path '*/.git/*' -prune \) \
  -not \( -path '*/.svn/*' -prune \) \
  -not \( -path '*/.bundle/*' -prune \) \
  -not \( -path '*/node_modules/*' -prune \) \
  -not \( -path '*/bower_components/*' -prune \) \
  -not \( -path '*/public/assets/*' -prune \) \
  -not \( -path '*/tmp*' -prune \) \
  -not \( -name '*.png' -prune \) \
  -not \( -name '*.gif' -prune \) \
  -not \( -name '*.jpg' -prune \) \
  -not \( -name '*.jpeg' -prune \) \
  -type f
